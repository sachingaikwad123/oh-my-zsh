
# General Settings: Sachin Gaikwad

# exports
export EDITOR=vim
export TMPDIR=~/tmp/
export PATH=$PATH:$HOME/bin
export SVN_EDITOR=$HOME/bin/svn-editor

# So that ctrl-q and ctrl-s dont mess up with my terminal and freeze it
# for putty
stty start ^_
stty stop ^_

# Include /opt/local/bin for Mac OSX ports in PATH
export PATH=$PATH:/opt/local/bin

